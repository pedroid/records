<!DOCTYPE html>
<html>
<head>
<meta charset="utf-8" />
</head>
  <body> 
    <form id="swim_record_new" method="POST" action="/admin/records/swim_record_new">
      <label>owner</label>
<select name="owner_id">
<% users_set.forEach(function(user){ %>
	<option value=<%= user.user_id %> > <%= user.user_name%></option>
	<% }); %>
</select>

	<label>item</label>
<select name="item_id">
<% for(item_id in items_set){ %>
	<option value=<%= item_id %> > <%= items_set[item_id]%></option>
	<% } %>
</select>
<br/>
<label>distance</label>
	<select name="distance_id">
	<% for(distance_id in distances_set){ %>
		<option value=<%= distance_id %> > <%= distances_set[distance_id]%></option>
		<% } %>
	</select>
<br/>
      <label>hour</label><input type="text" id="hour" name="hour" value="0"/><br/>
      <label>minute</label><input type="text" id="minute" name="minute" value="0"/><br/>
      <label>second</label><input type="text" id="second" name="second" value="0"/><br/>
      <label>millisecond</label><input type="text" id="millisecond" name="millisecond" value="0"/><br/>
      <label>authenticate</label>
	<select name="authenticate_id">
	<% for(authenticate_id in authenticates_set){ %>
		<option value=<%= authenticate_id %> > <%= authenticates_set[authenticate_id]%></option>
		<% } %>
	</select>
	<br/>
      <label>year</label><input type="text" id="year" name="year" value="2017"/><br/>
      <label>month</label><input type="text" id="month" name="month" value="7"/><br/>
      <label>date</label><input type="text" id="date" name="date" value="12"/><br/>
      <input type="submit" value="送出" />
    </form>
  </body>
</html>
